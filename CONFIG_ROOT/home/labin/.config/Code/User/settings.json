{
    "editor.renderControlCharacters": true,
    "files.trimTrailingWhitespace": true,
    "workbench.colorTheme": "GitHub Light",
    "editor.fontFamily": "'FiraCode Nerd Font', 'monospace', monospace",
    "terminal.integrated.fontFamily": "'FiraCode Nerd Font', 'monospace', monospace",
    "files.associations": {
        "*.djhtml": "html"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "prettier.tabWidth": 4,
    "workbench.colorCustomizations": {
        "terminal.background": "#f6f8fa",
        "terminal.foreground": "#373b41",
        "terminalCursor.background": "#f6f8fa",
        "terminalCursor.foreground": "#373b41",
        "terminal.ansiBlack": "#000000",
        "terminal.ansiRed": "#cc342b",
        "terminal.ansiGreen": "#2eb398",
        "terminal.ansiYellow": "#c0aa05",
        "terminal.ansiBlue": "#3971ed",
        "terminal.ansiMagenta": "#a36ac7",
        "terminal.ansiCyan": "#3971ed",
        "terminal.ansiWhite": "#373b41",
        "terminal.ansiBrightBlack": "#000000",
        "terminal.ansiBrightRed": "#cc342b",
        "terminal.ansiBrightGreen": "#2eb398",
        "terminal.ansiBrightYellow": "#ceb604",
        "terminal.ansiBrightBlue": "#3971ed",
        "terminal.ansiBrightMagenta": "#a36ac7",
        "terminal.ansiBrightCyan": "#3971ed",
        "terminal.ansiBrightWhite": "#000000",
        "editor.lineHighlightBackground": "#1073cf2d",
        "editor.lineHighlightBorder": "#9fced11f"
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "files.defaultLanguage": "${activeEditorLanguage}",
    "files.trimFinalNewlines": true,
    "workbench.sideBar.location": "right",
    "editor.linkedEditing": true,
    "terminal.integrated.profiles.linux": {
        "vscode-tmux-shell": {
            "path": "/home/labin/.local/bin/vscode-tmux-shell",
            "icon": "terminal-tmux"
        }
    },
    "terminal.integrated.defaultProfile.linux": "vscode-tmux-shell",
    "debug.console.fontFamily": "'FiraCode Nerd Font', 'monospace', monospace",
    "terminal.integrated.tabs.enabled": true,
    "sourcery.token": "user_IIhsJZQJpaxbkAWq71Vjs32bscCY1ignDp3bxYBccKwAEpS34fGanH2yzJ4",
    "terminal.external.linuxExec": "alacritty",
    "terminal.integrated.enableBell": true,
    "git.untrackedChanges": "separate",
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.iconTheme": "material-icon-theme",
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.renderWhitespace": "all",
    "gitlens.views.commits.files.layout": "tree",
    "prettier.vueIndentScriptAndStyle": true,
    "alignment.surroundSpace": {
        "colon": [0, 1],
        "assignment": [1, 1],
        "arrow": [1, 1],
        "comment": 2
    },
    "editor.inlineSuggest.enabled": true,
    "git.autofetch": true,
    "git.pruneOnFetch": true,
    "git.fetchOnPull": true,
    "github.copilot.enable": {
        "*": true,
        "yaml": false,
        "plaintext": true,
        "markdown": true
    },
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "githubPullRequests.queries": [
        {
            "label": "Waiting For My Review",
            "query": "is:open review-requested:${user}"
        },
        {
            "label": "Assigned To Me",
            "query": "is:open assignee:${user}"
        },
        {
            "label": "Created By Me",
            "query": "is:open author:${user}"
        }
    ],
    "gitlens.codeLens.scopes": ["document", "containers", "blocks"],
    "gitlens.views.repositories.includeWorkingTree": true,
    "cSpell.enabled": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.wordWrap": "off",
    "diffEditor.wordWrap": "off",
    "editor.guides.indentation": false,
    "editor.guides.bracketPairs": false,
    "editor.inlayHints.enabled": false,
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "blockman.n01LineHeight": 0,
    "editor.renderLineHighlight": "all",
    "workbench.editor.highlightModifiedTabs": true,
    "window.zoomLevel": -1.5
}
